# Environment Variables Configuration
# Copy this file to .env and replace with your actual values

# Search API Keys (optional - for web search functionality)
TAVILY_API_KEY=YOUR_TAVILY_API_KEY_HERE
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE
GOOGLE_CX=YOUR_GOOGLE_CUSTOM_SEARCH_ID_HERE

# Ollama Configuration
OLLAMA_API_BASE=http://localhost:11434
OLLAMA_NUM_PARALLEL=2
OLLAMA_MAX_LOADED_MODELS=1
OLLAMA_FLASH_ATTENTION=1
OLLAMA_KEEP_ALIVE=5m

# GitHub (optional - for GitHub integration)
GITHUB_PERSONAL_ACCESS_TOKEN=YOUR_GITHUB_TOKEN_HERE

# Note: Search functionality will work with just DuckDuckGo (no API key required)
# Tavily and Google are optional for enhanced search capabilities
